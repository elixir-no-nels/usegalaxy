[
  {rabbit, [{ssl_listeners, [5671]},
            {ssl_options, [{cacertfile,           "/etc/rabbitmq/fullchain.pem"},
                           {certfile,             "/etc/rabbitmq/cert.pem"}, 
                           {keyfile,              "/etc/rabbitmq/privkey.pem"}, 
                           {verify,               verify_none},
                           {fail_if_no_peer_cert, false}]}
           ]}
].
